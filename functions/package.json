{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve:database": "npm run build && firebase emulators:start --only functions,firestore",
    "serve:functions": "npm run build && firebase emulators:start --only functions",
